    _method.purpose
    _method.expression
     Evaluation
;
    With t as atom_type
 
    cnt =  0.
 
    Loop a  as  atom_site  {
 
       if ( a.type_symbol == t.symbol ) {
 
          cnt +=  a.occupancy * a.symmetry_multiplicity
    }  }
    _atom_type.number_in_cell =  cnt
;
